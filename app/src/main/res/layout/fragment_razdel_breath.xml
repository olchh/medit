<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".razdel_breath">

    <ImageView
        android:id="@+id/image_fon_razdel_beath"
        android:layout_width="415dp"
        android:layout_height="900dp"
        android:src="@drawable/fon6"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.378" />

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView8"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        app:cardBackgroundColor="#e7c697"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" >

        <ImageButton
            android:id="@+id/button_naz3"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_marginStart="10dp"
            android:layout_marginTop="10dp"
            android:background="@null"
            android:scaleType="fitXY"
            android:src="@drawable/button_naz1" />
    </androidx.cardview.widget.CardView>

    <ImageView
        android:id="@+id/imageViewDu"
        android:layout_width="260dp"
        android:layout_height="300dp"
        android:layout_marginTop="36dp"
        android:background="@null"
        android:scaleType="fitXY"
        android:src="@drawable/du6789"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cardView8" />

    <Button
        android:id="@+id/button_razdel_breath1"
        android:layout_width="135dp"
        android:layout_height="37dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="64dp"
        android:background="@drawable/button_bg"
        android:text="1 упражнение"
        android:textStyle="bold|italic"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageViewDu" />

    <Button
        android:id="@+id/button_razdel_breath3"
        android:layout_width="135dp"
        android:layout_height="37dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="15dp"
        android:background="@drawable/button_bg"
        android:text="3 упражнение"
        android:textStyle="bold|italic"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button_razdel_breath1" />

    <Button
        android:id="@+id/button_razdel_breath5"
        android:layout_width="135dp"
        android:layout_height="37dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="15dp"
        android:background="@drawable/button_bg"
        android:text="5 упражнение"
        android:textStyle="bold|italic"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button_razdel_breath3" />

    <Button
        android:id="@+id/button_razdel_breath7"
        android:layout_width="135dp"
        android:layout_height="37dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="15dp"
        android:background="@drawable/button_bg"
        android:text="7 упражнение"
        android:textStyle="bold|italic"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button_razdel_breath5" />

    <Button
        android:id="@+id/button_razdel_breath9"
        android:layout_width="135dp"
        android:layout_height="37dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="15dp"
        android:background="@drawable/button_bg"
        android:text="9 упражнение"
        android:textStyle="bold|italic"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button_razdel_breath7" />

    <Button
        android:id="@+id/button_razdel_breath2"
        android:layout_width="135dp"
        android:layout_height="37dp"
        android:layout_marginStart="48dp"
        android:layout_marginTop="64dp"
        android:background="@drawable/button_bg"
        android:text="2 упражнение"
        android:textStyle="bold|italic"
        app:layout_constraintStart_toEndOf="@+id/button_razdel_breath1"
        app:layout_constraintTop_toBottomOf="@+id/imageViewDu" />

    <Button
        android:id="@+id/button_razdel_breath4"
        android:layout_width="135dp"
        android:layout_height="37dp"
        android:layout_marginStart="48dp"
        android:layout_marginTop="15dp"
        android:background="@drawable/button_bg"
        android:text="4 упражнение"
        android:textStyle="bold|italic"
        app:layout_constraintStart_toEndOf="@+id/button_razdel_breath3"
        app:layout_constraintTop_toBottomOf="@+id/button_razdel_breath2" />

    <Button
        android:id="@+id/button_razdel_breath6"
        android:layout_width="135dp"
        android:layout_height="37dp"
        android:layout_marginStart="48dp"
        android:layout_marginTop="15dp"
        android:background="@drawable/button_bg"
        android:text="6 упражнение"
        android:textStyle="bold|italic"
        app:layout_constraintStart_toEndOf="@+id/button_razdel_breath5"
        app:layout_constraintTop_toBottomOf="@+id/button_razdel_breath4" />

    <Button
        android:id="@+id/button_razdel_breath8"
        android:layout_width="135dp"
        android:layout_height="37dp"
        android:layout_marginStart="48dp"
        android:layout_marginTop="15dp"
        android:background="@drawable/button_bg"
        android:text="8 упражнение"
        android:textStyle="bold|italic"
        app:layout_constraintStart_toEndOf="@+id/button_razdel_breath7"
        app:layout_constraintTop_toBottomOf="@+id/button_razdel_breath6" />

    <Button
        android:id="@+id/button_razdel_breath10"
        android:layout_width="135dp"
        android:layout_height="37dp"
        android:layout_marginStart="48dp"
        android:layout_marginTop="15dp"
        android:background="@drawable/button_bg"
        android:text="10 упражнение"
        android:textStyle="bold|italic"
        app:layout_constraintStart_toEndOf="@+id/button_razdel_breath9"
        app:layout_constraintTop_toBottomOf="@+id/button_razdel_breath8" />

    <TextView
        android:id="@+id/textViewRazdelBreath"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="25dp"
        android:text="комплекс дыхательных упражнений"
        android:textAllCaps="true"
        android:textColor="#000000"
        android:textStyle="bold|italic"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageViewDu" />
</androidx.constraintlayout.widget.ConstraintLayout>